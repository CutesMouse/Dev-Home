#include <iostream>
#include <algorithm>
using namespace std;
int main() {
	int n;
	int* data;
	while (cin >> n) {
		if (n == 0) return 0;
		data = new int[n];
		for (int i = 0; i < n; i++) cin >> data[i];
		sort(data, data + n);
		for (int i = 0;i < n;i++) cout << data[i]<< " ";
		cout << endl;
	}
}
