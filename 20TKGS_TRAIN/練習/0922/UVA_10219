#include <iostream>
#include <cmath>
using namespace std;
int main() {
	//C(a,b) = a!/b!(a-b)!
	int a,b;
	double d;
	while (cin >> a>>b) {
		d= 0;
		for (int i = a; i >b; i--) {
			d += log10(i);
		}
		for (int i = a-b; i > 0; i--) {
			d -= log10(i);
		}
		cout << (int)d + 1<< endl;
	}
}

