#include <iostream>

using namespace std;
int main() {
    int month;
    int day;
    while (cin >> month >> day) {
        int S = (month*2 + day)%3;
        switch (S) {
            case 0:
                cout << "普通" << "\n";
                break;
            case 1:
                cout << "吉" << "\n";
                break;
            case 2:
                cout << "大吉" << "\n";
                break;
            default:
                break;
        }
    }
}
